{
    "name": "markhatchell/cakephp3-easy-menus",
    "description": "An easy to use table based menu plugin for CakePHP3",
    "type": "cakephp-plugin",
    "require": {
        "php": ">=5.4.16",
        "cakephp/cakephp": "~3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "*"
    },
    "autoload": {
        "psr-4": {
            "EasyMenus\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EasyMenus\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    }
}
